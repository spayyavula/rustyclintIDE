{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "predev": "node -v && npm run test:client",
    "dev": "vite",
    "dev:fresh": "node scripts/clear-dev-cache.js && vite",
    "build": "vite build --emptyOutDir",
    "prebuild": "npm run test:client",
    "lint": "eslint --cache .",
    "preview": "vite preview",
    "prepreview": "npm run test:client",
    "test:client": "echo '✅ Client-side tests integrated into application'",
    "test:stripe": "echo '⚠️  Server-side Stripe tests require service role key'",
    "test:webhooks": "echo '⚠️  Webhook tests require service role key'",
    "test:db": "echo '⚠️  Database tests require service role key'",
    "test:all": "echo '⚠️  Full E2E tests require service role key - use in-app test suite instead'",
    "start": "concurrently \"npm run server\" \"npm run dev\"",
    "server": "node ./src/server/index.cjs",
    "prestart": "npm run test:client"
  },
  "dependencies": {
    "@stripe/stripe-js": "^5.0.0",
    "@supabase/supabase-js": "^2.39.8",
    "adm-zip": "^0.5.16",
    "express": "^5.1.0",
    "fs-extra": "^11.3.0",
    "localforage": "^1.10.0",
    "lucide-react": "^0.364.0",
    "node-fetch": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-query": "^3.39.3",
    "vite-plugin-compression": "^0.5.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.0",
    "@types/adm-zip": "^0.5.7",
    "@types/express": "^5.0.3",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.2.2",
    "autoprefixer": "^10.4.19",
    "concurrently": "^7.6.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "globals": "^13.0.0",
    "postcss": "^8.4.38",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.5",
    "vite": "^5.2.6",
    "workbox-window": "^7.0.1"
  }
}
